{
  "name": "fb-insta-downloader",
  "version": "1.0.0",
  "description": "Téléchargeur de vidéos Facebook/Instagram",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "clean": "rm -rf public/downloads/*",
    "setup": "./render-setup.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "yt-dlp-exec": "^2.0.0",
    "sanitize-filename": "^1.6.3",
    "express-rate-limit": "^6.8.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "eslint": "^9.0.0",
    "eslint-config-airbnb-base": "^17.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "render": {
    "installCommand": "./render-setup.sh && npm install",
    "startCommand": "npm start"
  }
}
