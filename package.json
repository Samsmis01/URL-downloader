{
  "name": "fb-insta-downloader",
  "version": "1.0.0",
  "description": "Téléchargeur de vidéos Facebook/Instagram",
  "main": "server.js",
  "scripts": {
    "preinstall": "echo 'Installation des dépendances en cours...'",
    "postinstall": "echo 'Installation terminée !'",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "clean": "rimraf videos/*",
    "build": "apt-get update && apt-get install -y python3-pip ffmpeg && pip3 install yt-dlp"
  },
  "dependencies": {
    "express": "^4.18.2",
    "youtube-dl-exec": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "chart.js": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "video",
    "downloader",
    "facebook",
    "instagram",
    "yt-dlp",
    "express"
  ],
  "author": "TonNom",
  "license": "MIT"
}
